<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="116.0" prefWidth="439.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="alarmClock.controller.AlertController">
   <center>
      <VBox alignment="CENTER" prefHeight="96.0" prefWidth="375.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Alert Description">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin></Label>
            <HBox alignment="BOTTOM_CENTER" prefHeight="95.0" prefWidth="348.0">
               <children>
                  <Button fx:id="closeButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#okButtonPressed" text="Ok">
                     <HBox.margin>
                        <Insets bottom="10.0" />
                     </HBox.margin></Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
